{
  "name": "daubjs",
  "version": "0.1.0",
  "type": "module",
  "description": "Powerful syntax highlighting on client or server",
  "main": "dist/daub.umd.cjs",
  "module": "src/all.js",
  "browser": "dist/daub.umd.cjs",
  "exports": {
    ".": "./lib/daub.js",
    "./umd": "./dist/daub.umd.cjs",
    "./umd/worker": "./dist/daub.worker.umd.cjs",
    "./all": "./lib/all.js",
    "./grammars/*": "./lib/grammars/*.js",
    "./grammars/*.js": "./lib/grammars/*.js",
    "./plugins/*": "./lib/plugins/*.js",
    "./plugins/*.js": "./lib/plugins/*.js"
  },
  "imports": {
    "#internal/*": "./src/*.js",
    "#internal/grammars/*": "./src/grammars/*.js",
    "#internal/plugins/*": "./src/plugins/*.js",
    "#internal/test/unit/helpers": "./test/unit/helpers.js"
  },
  "engines": {
    "node": ">= 17.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-transform-regenerator": "^7.4.5",
    "@babel/plugin-transform-spread": "^7.2.2",
    "@babel/preset-env": "^7.5.5",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-strip": "^3.0.2",
    "@rollup/plugin-terser": "^0.4.0",
    "ejs": "^3.1.8",
    "eslint": "^8.7.0",
    "eslint-plugin-unicorn": "^10.0.0",
    "express": "^4.18.2",
    "glob": "^8.1.0",
    "jasmine": "^4.5.0",
    "jest": "^29.4.1",
    "jsdom": "^21.1.0",
    "nodemon": "^2.0.20",
    "rollup": "^3.12.0"
  },
  "scripts": {
    "build": "rollup -c rollup.config.js --bundleConfigAsCjs",
    "dev": "rollup -w -c rollup.config.js --bundleConfigAsCjs",
    "test": "jasmine --config=jasmine.json",
    "test:server": "node ./test/server.js",
    "test:watch": "nodemon -e js --exec 'npm run test'"
  },
  "author": "Andrew Dupont",
  "license": "MIT",
  "volta": {
    "node": "18.13.0"
  }
}
