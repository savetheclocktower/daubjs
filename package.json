{
  "name": "daub",
  "version": "0.1.0",
  "type": "module",
  "description": "Syntax highlighting in the browser.",
  "main": "dist/daub.cjs.cjs",
  "module": "dist/daub.esm.js",
  "browser": "dist/daub.umd.cjs",
  "dependencies": {
    "core-js": "^3.2.1"
  },
  "exports": {
    ".": "./src/daub.js",
    "./cjs": "./dist/daub.cjs.cjs",
    "./umd": "./dist/daub.umd.cjs",
    "./worker": "./dist/daub.worker.umd.cjs",
    "./all": "./src/all.js",
    "./grammars/*": "./src/grammars/*.js",
    "./grammars/*.js": "./src/grammars/*.js",
    "./plugins/*": "./src/plugins/*.js",
    "./plugins/*.js": "./src/plugins/*.js"
  },
  "imports": {
    "#internal/*": "./src/*.js",
    "#internal/grammars/*": "./src/grammars/*.js",
    "#internal/plugins/*": "./src/plugins/*.js"
  },
  "engines": {
    "node": ">= 17.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-transform-regenerator": "^7.4.5",
    "@babel/plugin-transform-spread": "^7.2.2",
    "@babel/preset-env": "^7.5.5",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-strip": "^3.0.2",
    "@rollup/plugin-terser": "^0.4.0",
    "babel-eslint": "^10.0.2",
    "eslint": "^8.7.0",
    "eslint-plugin-unicorn": "^10.0.0",
    "rollup": "^3.12.0"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -w -c rollup.config.js --bundleConfigAsCjs"
  },
  "author": "Andrew Dupont",
  "license": "MIT",
  "volta": {
    "node": "18.13.0"
  }
}
